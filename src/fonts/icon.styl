@font-face
  font-family: 'icomoon';
  src: url('icomoon.eot?ckh9w9');
  src: url('icomoon.eot?ckh9w9#iefix') format('embedded-opentype'),
          url('icomoon.ttf?ckh9w9') format('truetype'),
          url('icomoon.woff?ckh9w9') format('woff'),
          url('icomoon.svg?ckh9w9#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;

[icon]
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;

@-webkit-keyframes cicrle
  0%
    transform rotate(0)
  100%
    transform rotate(360deg)

[icon^="home"]:before
  content "\e900"

[icon^="pencil"]:before
  content "\e905"

[icon^="dillar"]:before
  content "\e93b"

[icon^="undo"]:before
  content "\e965"

[icon^="redo"]:before
  content "\e966"

[icon^="plus"]:before
  content "\ea0a"

[icon^="minus"]:before
  content "\ea0b"

[icon^="cross"]:before
  content "\ea0f"

[icon^="checkmark1"]:before
  content "\ea10"

[icon^="arrow-right"]:before
  content "\ea34"

[icon^="arrow-left"]:before
  content "\ea38"

[icon^="spinner1"]:before
  content "\e97c"
  display inline-block
  animation cicrle 1s linear infinite

[icon^="spinner2"]:before
  content "\e97f"
  display inline-block
  animation cicrle 1s linear infinite

[icon^="spinner3"]:before
  content "\e980"
  display inline-block
  animation cicrle 1s linear infinite

[icon^="spinner4"]:before
  content "\e983"
  display inline-block
  animation cicrle 1s linear infinite

[icon^="icon-first"]:before
  content "\ea21"
  font-size 15px

[icon^="icon-last"]:before
  content "\ea22"
  font-size 15px

[icon^="icon-previous"]:before
  content "\ea23"
  font-size 15px


[icon^="icon-next"]:before
  content "\ea24"
  font-size 15px
